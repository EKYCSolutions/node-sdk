{
  "name": "ekycsolutions",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "clean": "lerna run clean",
    "ts-compile": "lerna run tsc",
    "prepublish": "lerna run prepublish",
    "publish": "lerna publish --no-private --no-push",
    "unpublish": "npm unpublish",
    "publish.local": "yarn prepublish && lerna exec 'npm publish --registry http://localhost:4873'",
    "unpublish.local": "lerna exec 'npm unpublish --force --registry http://localhost:4873'"
  },
  "dependencies": {
    "got": "^12.0.1"
  },
  "version": "0.0.7"
}
